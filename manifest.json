{
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://unpkg.com; object-src 'self'",
  "name": "Firefox Wallpaper",
  "version": "1.0",
  "description": "Custom new tab with wallpaper support.",
  "permissions": ["storage"],
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  }
}
